{
  "device": "DigitalIoTBroker-Server",
  "ip": "192.168.20.2",
  "role": "MQTT Broker and RTSP Relay",
  "interfaces": {
    "eth0": "192.168.20.0/24 (OpenvSwitch-1)"
  },
  "services": [
    {
      "name": "MQTT Broker",
      "command": "python3 mqttbroker.py",
      "port": 1883,
      "topics": {
        "publishes": ["sensors/temp", "sensors/humidity"],
        "subscribes": ["sensors/#"]
      }
    },
    {
      "name": "MediaMTX RTSP",
      "command": "./mediamtx",
      "port": 8554,
      "streams": {
        "input": "rtsp://DigitalIPCamera/stream1",
        "output": "rtsp://DigitalIoTBroker-Server/live"
      }
    }
  ],
  "connected_devices": [
    "IoTConsumer",
    "TemperatureHumiditySensor",
    "DigitalIPCamera",
    "DigitalRouter"
  ],
  "attack_relevance": [
    "MQTT DoS attack disrupts all telemetry",
    "RTSP exploit blocks video streaming"
  ]
}
